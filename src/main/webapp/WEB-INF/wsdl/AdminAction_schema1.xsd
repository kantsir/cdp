<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://cdp.epam.com/ws/admin" xmlns:tns="http://cdp.epam.com/ws/admin" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="addNewClient" type="tns:addNewClient"/>

  <xs:element name="addNewClientResponse" type="tns:addNewClientResponse"/>

  <xs:element name="getAllAccounts" type="tns:getAllAccounts"/>

  <xs:element name="getAllAccountsResponse" type="tns:getAllAccountsResponse"/>

  <xs:element name="getAllActivities" type="tns:getAllActivities"/>

  <xs:element name="getAllActivitiesResponse" type="tns:getAllActivitiesResponse"/>

  <xs:element name="getAllClients" type="tns:getAllClients"/>

  <xs:element name="getAllClientsResponse" type="tns:getAllClientsResponse"/>

  <xs:element name="getAllDeposites" type="tns:getAllDeposites"/>

  <xs:element name="getAllDepositesResponse" type="tns:getAllDepositesResponse"/>

  <xs:element name="getAllSystemProperty" type="tns:getAllSystemProperty"/>

  <xs:element name="getAllSystemPropertyResponse" type="tns:getAllSystemPropertyResponse"/>

  <xs:element name="getClientAccount" type="tns:getClientAccount"/>

  <xs:element name="getClientAccountResponse" type="tns:getClientAccountResponse"/>

  <xs:element name="getClientActivities" type="tns:getClientActivities"/>

  <xs:element name="getClientActivitiesResponse" type="tns:getClientActivitiesResponse"/>

  <xs:element name="getClientById" type="tns:getClientById"/>

  <xs:element name="getClientByIdResponse" type="tns:getClientByIdResponse"/>

  <xs:element name="getClientDeposites" type="tns:getClientDeposites"/>

  <xs:element name="getClientDepositesResponse" type="tns:getClientDepositesResponse"/>

  <xs:element name="getClientsByAdress" type="tns:getClientsByAdress"/>

  <xs:element name="getClientsByAdressResponse" type="tns:getClientsByAdressResponse"/>

  <xs:element name="getClientsByEmail" type="tns:getClientsByEmail"/>

  <xs:element name="getClientsByEmailResponse" type="tns:getClientsByEmailResponse"/>

  <xs:element name="getClientsByName" type="tns:getClientsByName"/>

  <xs:element name="getClientsByNameResponse" type="tns:getClientsByNameResponse"/>

  <xs:element name="getClientsByPhone" type="tns:getClientsByPhone"/>

  <xs:element name="getClientsByPhoneResponse" type="tns:getClientsByPhoneResponse"/>

  <xs:element name="getSystemPropertyValueByKey" type="tns:getSystemPropertyValueByKey"/>

  <xs:element name="getSystemPropertyValueByKeyResponse" type="tns:getSystemPropertyValueByKeyResponse"/>

  <xs:element name="removeClient" type="tns:removeClient"/>

  <xs:element name="removeClientResponse" type="tns:removeClientResponse"/>

  <xs:element name="updateClientDetails" type="tns:updateClientDetails"/>

  <xs:element name="updateClientDetailsResponse" type="tns:updateClientDetailsResponse"/>

  <xs:element name="updateSystemPropety" type="tns:updateSystemPropety"/>

  <xs:element name="updateSystemPropetyResponse" type="tns:updateSystemPropetyResponse"/>

  <xs:complexType name="addNewClient">
    <xs:sequence>
      <xs:element name="client" type="tns:client" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="client">
    <xs:sequence>
      <xs:element name="account" type="tns:account" minOccurs="0"/>
      <xs:element name="activities" type="tns:activity" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="address" type="xs:string" minOccurs="0"/>
      <xs:element name="clientActiveState" type="tns:clientActiveState" minOccurs="0"/>
      <xs:element name="clientName" type="xs:string" minOccurs="0"/>
      <xs:element name="comment" type="xs:string" minOccurs="0"/>
      <xs:element name="deposites" type="tns:deposit" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="email" type="xs:string" minOccurs="0"/>
      <xs:element name="password" type="xs:string" minOccurs="0"/>
      <xs:element name="phone" type="xs:string" minOccurs="0"/>
      <xs:element name="type" type="tns:clientType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="account">
    <xs:sequence>
      <xs:element name="balance" type="xs:double" minOccurs="0"/>
      <xs:element name="client" type="tns:client" minOccurs="0"/>
      <xs:element name="comment" type="xs:string" minOccurs="0"/>
      <xs:element name="creditLimit" type="xs:double" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="activity">
    <xs:sequence>
      <xs:element name="activityDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="amount" type="xs:double" minOccurs="0"/>
      <xs:element name="client" type="tns:client" minOccurs="0"/>
      <xs:element name="commision" type="xs:double" minOccurs="0"/>
      <xs:element name="description" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="deposit">
    <xs:sequence>
      <xs:element name="balance" type="xs:double" minOccurs="0"/>
      <xs:element name="client" type="tns:client" minOccurs="0"/>
      <xs:element name="closingDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="openingDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="type" type="tns:depositType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="addNewClientResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getClientById">
    <xs:sequence>
      <xs:element name="clientId" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientByIdResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientAccount">
    <xs:sequence>
      <xs:element name="clientId" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientAccountResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:account" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="removeClient">
    <xs:sequence>
      <xs:element name="clientId" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="removeClientResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getClientsByPhone">
    <xs:sequence>
      <xs:element name="clientPhone" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByPhoneResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getAllSystemProperty">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllSystemPropertyResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:property" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="property">
    <xs:sequence>
      <xs:element name="propertyKey" type="xs:string" minOccurs="0"/>
      <xs:element name="propertyValue" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByName">
    <xs:sequence>
      <xs:element name="clientName" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByNameResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByAdress">
    <xs:sequence>
      <xs:element name="clientAddress" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByAdressResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getAllClients">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllClientsResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getAllActivities">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllActivitiesResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:activity" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientActivities">
    <xs:sequence>
      <xs:element name="clientId" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientActivitiesResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:activity" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="updateClientDetails">
    <xs:sequence>
      <xs:element name="client" type="tns:client" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="updateClientDetailsResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllDeposites">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllDepositesResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:deposit" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientDeposites">
    <xs:sequence>
      <xs:element name="clientId" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientDepositesResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:deposit" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="updateSystemPropety">
    <xs:sequence>
      <xs:element name="property" type="tns:property" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="updateSystemPropetyResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getClientsByEmail">
    <xs:sequence>
      <xs:element name="clientEmail" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getClientsByEmailResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:client" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getSystemPropertyValueByKey">
    <xs:sequence>
      <xs:element name="key" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getSystemPropertyValueByKeyResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:property" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getAllAccounts">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getAllAccountsResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:account" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="clientActiveState">
    <xs:restriction base="xs:string">
      <xs:enumeration value="DELETED"/>
      <xs:enumeration value="ACTIVE"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="depositType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="LONG"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="clientType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="REGULAR"/>
      <xs:enumeration value="GOLD"/>
      <xs:enumeration value="PLATINUM"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>

